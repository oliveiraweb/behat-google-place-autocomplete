{
  "name": "medology/behat-google-place-autocomplete",
  "type": "behat-extension",
  "description": "Mink extension for Google Place Autocomplete",
  "license": "MIT",
  "require": {
    "php": ">=5.6",
    "behat/behat": "^3.0",
    "medology/flexible-mink": "~2.0",
    "behat/mink-extension": "^2.0",
    "symfony/config": "^4.0"
  },
  "require-dev":{
    "vlucas/phpdotenv": "^5.3",
    "behat/mink-selenium2-driver": "*",
    "chekote/behat-retry-extension": "^1.1"
  },
  "autoload": {
    "psr-0": {
      "": [
        "src/"
      ]
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "discard-changes": true
  }
}
